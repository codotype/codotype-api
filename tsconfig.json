{
    "compilerOptions": {
        "target": "ES2018",
        "module": "commonjs",
        "lib": ["es2016", "dom", "es2017.object", "es2017.string"],
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": false,
        "inlineSourceMap": true,
        "inlineSources": true,
        "experimentalDecorators": true,
        "strictPropertyInitialization": false,
        "jsx": "react"
    },
    "exclude": [
        "./src/generate-endpoint/plugin",
        "./src/plugins-endpoint/plugin",
        "./src/preview-endpoint/plugin",
        "cdk.out"
    ]
}
